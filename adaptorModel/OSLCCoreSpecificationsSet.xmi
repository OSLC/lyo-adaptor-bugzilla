  <domainSpecifications name="OSLC_CORE" namespaceURI="http://open-services.net/ns/core" namespacePrefix="//@domainPrefixes.0">
    <resourceProperties title="" name="discussedBy" occurs="zeroOrOne" valueType="Resource" representation="either" description="A series of notes and comments about this change request."/>
    <resourceProperties title="" name="instanceShape" occurs="zeroOrOne" valueType="Resource" description="Resource Shape that provides hints as to resource property value-types and allowed values."/>
